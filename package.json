{
  "main": "./dist/extension.cjs",
  "publisher": "MolikoDeveloper",
  "name": "vs-bruno",
  "version": "0.0.2",
  "description": "a bruno files compatible api client.",
  "displayName": "VS-Bruno",
  "icon": "icon.png",
  "vsce": {
    "dependencies": true,
    "yarn": false
  },
  "dependencies": {
    "@codemirror/autocomplete": "6.18.6",
    "@codemirror/lang-javascript": "6.2.3",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/lang-xml": "6.1.0",
    "@codemirror/lint": "^6.8.5",
    "@monaco-editor/react": "^4.7.0",
    "@rollup/browser": "^4.40.2",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-virtual": "^3.0.2",
    "@rollup/pluginutils": "5.1.4",
    "@uiw/codemirror-theme-vscode": "4.23.12",
    "@uiw/codemirror-themes": "^4.23.12",
    "@uiw/react-codemirror": "4.23.12",
    "@usebruno/lang": "0.22.0",
    "dompurify": "3.2.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rollup": "4.40.2",
    "source-map": "^0.7.4",
    "style-mod": "4.1.2",
    "tar": "7.4.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.5",
    "@types/bun": "^1.2.12",
    "@types/dompurify": "3.2.0",
    "@types/marked": "6.0.0",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "@types/vscode": "1.99.1",
    "@types/vscode-webview": "1.57.5",
    "@vscode/vsce": "3.3.2",
    "bun-plugin-tailwind": "0.0.15",
    "esbuild": "0.25.4",
    "esbuild-style-plugin": "1.6.3",
    "tailwindcss": "4.1.5",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-android-arm-eabi": "4.40.2",
    "@rollup/rollup-android-arm64": "4.40.2",
    "@rollup/rollup-darwin-arm64": "4.40.2",
    "@rollup/rollup-darwin-x64": "4.40.2",
    "@rollup/rollup-freebsd-arm64": "4.40.2",
    "@rollup/rollup-freebsd-x64": "4.40.2",
    "@rollup/rollup-linux-arm-gnueabihf": "4.40.2",
    "@rollup/rollup-linux-arm-musleabihf": "4.40.2",
    "@rollup/rollup-linux-arm64-gnu": "4.40.2",
    "@rollup/rollup-linux-arm64-musl": "4.40.2",
    "@rollup/rollup-linux-loongarch64-gnu": "4.40.2",
    "@rollup/rollup-linux-powerpc64le-gnu": "4.40.2",
    "@rollup/rollup-linux-riscv64-gnu": "4.40.2",
    "@rollup/rollup-linux-riscv64-musl": "4.40.2",
    "@rollup/rollup-linux-s390x-gnu": "4.40.2",
    "@rollup/rollup-linux-x64-gnu": "4.40.2",
    "@rollup/rollup-linux-x64-musl": "4.40.2",
    "@rollup/rollup-win32-arm64-msvc": "4.40.2",
    "@rollup/rollup-win32-ia32-msvc": "4.40.2",
    "@rollup/rollup-win32-x64-msvc": "4.40.2"
  },
  "type": "module",
  "scripts": {
    "dev": "echo 'Open this directory in VSCode and then run your extension with F5 or `Run and Debug > Run Extension`!'",
    "build": "bun run ./scripts/build-with-esbuild.ts",
    "watch": "bun run ./scripts/watch-with-esbuild.ts",
    "prelude": "bun ",
    "vsce-package": "bun run vsce package",
    "clean": "rm -r ./dist",
    "build-rollup": "bunx rollup -c rollup.config.js",
    "build-all": "bun run build && bun run build-rollup",
    "package": "bun run build && bun run vsce-package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MolikoDeveloper/VsBruno"
  },
  "contributes": {
    "commands": [
      {
        "command": "vs-bruno.bundle.req",
        "title": "Debug Script",
        "category": "vs-bruno"
      },
      {
        "command": "vs-bruno.disable.rollup",
        "title": "Disable Scripts",
        "category": "vs-bruno"
      },
      {
        "command": "vs-bruno.enable.rollup",
        "title": "Enable Scripts",
        "category": "vs-bruno"
      }
    ],
    "keybindings": [],
    "customEditors": [
      {
        "viewType": "vs-bruno.collectionEditor",
        "displayName": "Bruno Collection Editor",
        "priority": "option",
        "selector": [
          {
            "filenamePattern": "**/collection.bru"
          }
        ]
      },
      {
        "viewType": "vs-bruno.environmentEditor",
        "displayName": "Bruno environment Editor",
        "priority": "option",
        "selector": [
          {
            "filenamePattern": "**/environments/**/*.bru"
          }
        ]
      },
      {
        "viewType": "vs-bruno.bruEditor",
        "displayName": "Bruno Editor",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "**/*.bru"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "bru",
        "extensions": [
          ".bru"
        ],
        "aliases": [
          "BRU",
          "bru"
        ],
        "icon": {
          "light": "bruno.svg",
          "dark": "bruno.svg"
        }
      }
    ],
    "configuration": [
      {
        "title": "VS-Bruno Settings",
        "properties": {
          "vs-bruno.moduleResolutionOrder": {
            "type": "array",
            "order": 1,
            "minItems": 1,
            "items": {
              "type": "string",
              "enum": [
                "global",
                "workspace",
                "extension"
              ]
            },
            "default": [
              "extension",
              "workspace",
              "global"
            ],
            "description": "Define la prioridad para la resolucion de modulos del sandbox."
          },
          "vs-bruno.useModuleResolution": {
            "type": "boolean",
            "order": 0,
            "default": true,
            "description": "Habilita o deshabilita la resolución de módulos en el sandbox de VS-Bruno según el orden definido en `moduleResolutionOrder` (global → workspace → extensión)."
          }
        }
      }
    ]
  },
  "engines": {
    "vscode": "^1.99.3"
  }
}